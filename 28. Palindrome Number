class Solution {
    public boolean isPalindrome(int x) {
        int a=x,i,f=0;
        if(x<0)
            return false;
        while(a>0)
        {
            i=a%10;
            f=(f*10)+i;
            a=a/10;
        }
        if(f==x)
            return true;
        else
            return false;
        
    }
}

class Solution {
    public int[] twoSum(int[] nums, int target) {
        int i,j,flag=0;
        int s=nums.length;
        for(i=0;i<s;i++)
        {
            for(j=i+1;j<s;j++)
            {
                if(nums[i]+nums[j]==target)
                {
                    return new int[]{i,j};
                }
                
            }
        }
       
            return new int[]{};           
        
    }
}
